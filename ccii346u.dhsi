;*
public procedure Bloc_TotalGeneral_1_Ap		
;après total général
1	i		3,0
beginp
   X5.MTSLDDBTB(9) = X5.MTSLDDBTB(9) - X5.MTSLDCRTB(9)
   X5.MTSLDCRTB(9) = 0
   if X5.MTSLDDBTB(9) < 0
      X5.MTSLDCRTB(9) = X5.MTSLDDBTB(9) *  - 1
      X5.MTSLDDBTB(9) = 0
   endif
   X5.MTSLDDBTB(10) = X5.MTSLDDBTB(10) - X5.MTSLDCRTB(10)
   X5.MTSLDCRTB(10) = 0
   if X5.MTSLDDBTB(10) < 0
      X5.MTSLDCRTB(10) = X5.MTSLDDBTB(10) *  - 1
      X5.MTSLDDBTB(10) = 0
   endif
   X5.MTREPTB(11) = X5.MTREPTB(10) + X5.MTREPTB(9)
   X5.MTDBTB(11) = X5.MTDBTB(10) + X5.MTDBTB(9)
   X5.MTCRTB(11) = X5.MTCRTB(10) + X5.MTCRTB(9)
   X5.MTSLDDBTB(11) = X5.MTREPTB(11) + X5.MTDBTB(11) - X5.MTCRTB(11)
   if X5.MTSLDDBTB(11) < 0
      X5.MTSLDCRTB(11) = X5.MTSLDDBTB(11) *  - 1
      X5.MTSLDDBTB(11) = 0
   endif
   for i = 4 TO 10
      X5.MTREPTB(14) = X5.MTREPTB(14) + X5.MTREPTB(i)
      X5.MTDBTB(14) = X5.MTDBTB(14) + X5.MTDBTB(i)
      X5.MTCRTB(14) = X5.MTCRTB(14) + X5.MTCRTB(i)
   next
   for i = 12 TO 13                                   ;fiche 925
      X5.MTREPTB(14) = X5.MTREPTB(14) + X5.MTREPTB(i) ;fiche 925
      X5.MTDBTB(14) = X5.MTDBTB(14) + X5.MTDBTB(i)    ;fiche 925
      X5.MTCRTB(14) = X5.MTCRTB(14) + X5.MTCRTB(i)    ;fiche 925
   next
   X5.MTSLDDBTB(14) = X5.MTREPTB(14) + X5.MTDBTB(14) - X5.MTCRTB(14)
   if X5.MTSLDDBTB(14) < 0
      X5.MTSLDCRTB(14) = X5.MTSLDDBTB(14) *  - 1
      X5.MTSLDDBTB(14) = 0
   endif
endp

;*
public procedure Bloc_RecapitulatifDeGranlivr_1_Av				
;avant récapitulatif
1	i		3,0
beginp
   for i = 1 to 14
      X5.CHOIX1(i) = 3
      if X5.MTREPTB(i) < 0
         X5.CHOIX1(i) = 2
         X5.MTREPTB(i) = X5.MTREPTB(i) * ( - 1)
      else
         if X5.MTREPTB(i) > 0
            X5.CHOIX1(i) = 1
         endif
      endif
   next
endp

