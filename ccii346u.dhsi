include "cctc000.dhsp"

public record CCFDD.dhsd x4
public record CCFDD.dhsd x5
Public Record "ccfdd.dhsd" c3 c3
Public Record "ccfdd.dhsd" ca ca
Public Record "gtfdd.dhsd" xp xp

;*
;* Fonctions et procédures
;*

;*
Procedure DBCR (inx)

1	inx	N

beginp
	X5.MT = X4.BALMTREP(inx)
	C3XQ.CHOIX(6) = 3
	X4.MTSLD1 = X4.BALMTSLDDB(inx) - X4.BALMTSLDCR(inx)
	If X4.BALMTREP(inx) < 0
		C3XQ.CHOIX(6) = 2
		X4.BALMTREP(inx) = X4.BALMTREP(inx) * (-1)
	Else
		If X4.BALMTREP(inx) > 0
			C3XQ.CHOIX(6) = 1
		EndIf
	EndIf
EndP ; DBCR

;*
;* Séquences
;*

;*
public procedure Bloc_EditionCompte_1_Av				
;avant édition ligne détail
beginp
   X5.MT = X4.MTREP
   if C3XQ.Choix(4) <> 2 or (X4.MtRep = 0 and X4.MtDb = 0 and X4.MtCr = 0 and X4.FlZero) 	; pas cpte zéro
      HARMONY.RETOUR = 1
   endif
   X4.MTSLD1 = X4.MTREP + X4.MTDB - X4.MTCR
   C3XQ.CHOIX(6) = 3
   if X4.MTREP < 0
      C3XQ.CHOIX(6) = 2
      X4.MTREP = X4.MTREP * ( - 1)
   else
      if X4.MTREP > 0
         C3XQ.CHOIX(6) = 1
      endif
   endif

   C3XQ.CHOIX(7) = 3
   if X4.MTDEVSLD < 0
      C3XQ.CHOIX(7) = 2
      X4.MTDEVSLD = X4.MTDEVSLD * ( - 1)
   else
      if X4.MTDEVSLD > 0
         C3XQ.CHOIX(7) = 1
      endif
   endif
endp

;*
public procedure Bloc_EditionCompte_2_Av				
;avant édition total devise
beginp
   if C3XQ.Choix(4) <> 2 or (X4.MtRep = 0 and X4.MtDb = 0 and X4.MtCr = 0 and X4.FlZero)	\ 	;pas cpte zéro
   or (X4.MTDEVREP = 0 and X4.MTDEVDB = 0 and X4.MTDEVCR = 0) 											;pas total devise zéro
      HARMONY.RETOUR = 1
   endif
   C3XQ.CHOIX(6) = 3
   X5.MT = X4.MTDEVREP
   if X4.MTDEVREP < 0
      C3XQ.CHOIX(6) = 2
      X4.MTDEVREP = X4.MTDEVREP * ( - 1)
   else
      if X4.MTDEVREP > 0
         C3XQ.CHOIX(6) = 1
      endif
   endif

   X5.MT = X4.MTDEVSLD
   if X4.MTDEVSLD < 0
      X4.MTDEVSLD = X4.MTDEVSLD * ( - 1)
      X4.MTSLDCR = X4.MTDEVSLD
      X4.MTSLDDB = 0
   else
      X4.MTSLDDB = X4.MTDEVSLD
      X4.MTSLDCR = 0
   endif
endp

;*
public procedure Bloc_TotalGroupe_1_Av			
;avant total groupe
beginp
   if C3XQ.CHOIX(4) > 2                   ;que les classes
      HARMONY.RETOUR = 1
   endif
   if X4.FLRUP = 0                      ;pas de compte édité
      HARMONY.RETOUR = 1
   endif
   DBCR(1)
endp

