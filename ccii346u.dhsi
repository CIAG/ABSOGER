;*
; CIAG Août 2016 - PJO : Balance Sortie Excel 
;*

Public Record "ccfdd.dhsd" c3 c3
Public Record "a5dd.dhsd" mz mz
Public Record "ccfdd.dhsd" x4 x4
Public Record "ccfdd.dhsd" xq c3xq
Public Record "ccfdd.dhsd" x5 x5
Public Record "ccfdd.dhsd" ca ca
Public Record "gtfdd.dhsd" xp xp

module "yoffice.dhop"
1	  Wt_EXCEL_repert  256 
1	  Wt_EXCEL_fichier 256
1	  Wt_EXCEL_feuille 256
1    Wt_Statut          1,0
1    Wt_LIG             8,0
1 	  Wt_Premiere_Fois	1
1	  Wt_BALANCE		  50
1    Wi                 3,0


;*
Private Procedure Bloc_EditionCompte_1_Ap
;*
; CIAG Août 2016 - PJO : Balance Sortie Excel 
;*

Beginp

	If c3xq.Choix(9) = 2
		If Wt_Premiere_Fois = ' ' 
			Wt_Premiere_Fois = 'N' 
			Office_Begin
			;* BTN le 07/09/2016 - DDM 91 : NE PLUS SPECIFIER DE REPERTOIRE NI DE NOM
;			Wt_EXCEL_repert = "c:\divalto_fichiers\"
;			If MZ.Dos = 100
;		  		Wt_EXCEL_fichier = "balance_absoger.xlsx"
;			Elsif MZ.Dos = 200
;		  		Wt_EXCEL_fichier = "balance_gerbaud.xlsx"
;			Elsif MZ.Dos = 300
;		  		Wt_EXCEL_fichier = "balance_cts.xlsx"
;			Endif

		;	Wt_EXCEL_feuille = 'BALANCE'

		;	Wt_Statut = Office_OpenCalc(Wt_EXCEL_repert,Wt_EXCEL_fichier, Wt_EXCEL_feuille , 1, , ,1)				; ouverture d'une feuille excel
			Wt_Statut = Office_OpenCalc(" "," " , Wt_EXCEL_feuille , 1, , ,1)				; ouverture d'une feuille excel		
			; Titre 
			Wt_LIG =  1 
			If c3xq.Choix(8) = 1 
				Wt_BALANCE = "Générale"
			Elsif c3xq.Choix(8) = 2
				Wt_BALANCE = "Client"
			Elsif c3xq.Choix(8) = 3
				Wt_BALANCE = "Fournisseur"
			Elsif c3xq.Choix(8) = 4
				Wt_BALANCE = "Personnel"
			Else
				Wt_BALANCE =  c3xq.Choix(8)
			Endif
			Office_WriteString(Wt_LIG, 1, "Balance " & Wt_BALANCE )
			Office_WriteFormat(Wt_LIG, 3, sdate(c3xq.PerDDt, '/'), "DATE")
			Office_WriteFormat(Wt_LIG, 4, sdate(c3xq.PerFDt, '/'), 'DATE')
			; Entête Colonne 
			Wt_LIG = Wt_LIG + 1 
			Office_WriteString(Wt_LIG, 1, "COMPTE")
			Office_WriteString(Wt_LIG, 2, "INTITULE")
			Office_WriteString(Wt_LIG, 3, "REPORT")

			Office_WriteString(Wt_LIG, 5, "MOUVEMENTS-DEBIT")
			Office_WriteString(Wt_LIG, 6, "MOUVEMENTS-CREDIT")
			Office_WriteString(Wt_LIG, 7, "SOLDE-DEBIT")
			Office_WriteString(Wt_LIG, 8, "SOLDE-CREDIT")
	
		Endif
		
		; Ecriture feuille Excel 
		Wt_LIG = Wt_LIG + 1 
		Office_WriteString(Wt_LIG, 1, C3.Cpt)
		Office_WriteString(Wt_LIG, 2, C3.Lib)
		Office_WriteFormat(Wt_LIG, 3, X4.MtRep, "Numeric", 2)
		If C3xq.Choix(6) = 2
			Office_WriteString(Wt_LIG, 4, "Cr")
		Elsif C3xq.Choix(6) = 1
			Office_WriteString(Wt_LIG, 4, "Db")
		Endif
		Office_WriteFormat(Wt_LIG, 5, X4.MtDb, "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 6, X4.MtCr, "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 7, X4.MtSldDb, "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 8, X4.MtSldCr, "Numeric", 2)
	Endif
Endp




;*
Procedure Imprimer_Ligne_Recap
;*
; CIAG Août 2016 - PJO : Balance Sortie Excel 
;*

Beginp
		Office_WriteFormat(Wt_LIG, 3, X5.MtRepTb(Wi), "Numeric", 2)
		If x5.Choix1(Wi) = 2
			Office_WriteString(Wt_LIG, 4, "Cr")
		Elsif x5.Choix1(Wi) = 1
			Office_WriteString(Wt_LIG, 4, "Db")
		Endif
		Office_WriteFormat(Wt_LIG, 5, X5.MtDbTb(Wi), "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 6, X5.MtCrTb(Wi), "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 7, X5.MtSldDbTb(Wi), "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 8, X5.MtSldCrTb(Wi), "Numeric", 2)
Endp




;*
Procedure Bloc_RecapitulatifDeGranlivr_1_Ap
;*
; CIAG Août 2016 - PJO : Balance Sortie Excel 
;*

Beginp

	Standard.Bloc_RecapitulatifDeGranlivr_1_Ap
	
	If c3xq.Choix(9) = 2
		
			; Titre 
			Wt_LIG =  Wt_LIG + 5  
			Office_WriteString(Wt_LIG, 1, "Balance " & Wt_BALANCE )
			Office_WriteFormat(Wt_LIG, 3, sdate(c3xq.PerDDt, '/'), "DATE")
			Office_WriteFormat(Wt_LIG, 4, sdate(c3xq.PerFDt, '/'), 'DATE')
			; Entête Colonne 
			Wt_LIG = Wt_LIG + 1 
			Office_WriteString(Wt_LIG, 2, "RECAPITULATIF")
			Office_WriteString(Wt_LIG, 3, "REPORT")

			Office_WriteString(Wt_LIG, 5, "MOUVEMENTS-DEBIT")
			Office_WriteString(Wt_LIG, 6, "MOUVEMENTS-CREDIT")
			Office_WriteString(Wt_LIG, 7, "SOLDE-DEBIT")
			Office_WriteString(Wt_LIG, 8, "SOLDE-CREDIT")

		;* COMPTES AUXILIAIRES 
			Wt_LIG = Wt_LIG + 2 
			Office_WriteString(Wt_LIG, 2, "COMPTES AUXILIAIRES")
		;*
			Wt_LIG = Wt_LIG + 2 
			Wi = 1
			Office_WriteString(Wt_LIG, 2, "CLIENTS")
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 2
			Office_WriteString(Wt_LIG, 2, "FOURNISSEURS")
			Imprimer_Ligne_Recap 
  		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 3
			Office_WriteString(Wt_LIG, 2, "AUTRES")
			Imprimer_Ligne_Recap 

		;* COMPTES DE BILAN  
			Wt_LIG = Wt_LIG + 2 
			Office_WriteString(Wt_LIG, 2, "COMPTES DE BILAN")
		;*
			Wt_LIG = Wt_LIG + 2 
			Wi = 4
			Office_WriteString(Wt_LIG, 2, "Comptes de Capitaux")
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 5
			Office_WriteString(Wt_LIG, 2, "Comptes d'immobilisation")
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 6
			Office_WriteString(Wt_LIG, 2, "Comptes Stock et En-cours")
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 7
			Office_WriteString(Wt_LIG, 2, "Comptes de Tiers")
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 8
			Office_WriteString(Wt_LIG, 2, "Comptes Financiers")
			Imprimer_Ligne_Recap 
	
		;* COMPTES DE GESTION  
			Wt_LIG = Wt_LIG + 2 
			Office_WriteString(Wt_LIG, 2, "COMPTES DE GESTION")
		;*
			Wt_LIG = Wt_LIG + 2 
			Wi = 9
			Office_WriteString(Wt_LIG, 2, "Comptes de Charges")
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 10
			Office_WriteString(Wt_LIG, 2, "Comptes de Produits")
			Imprimer_Ligne_Recap 

		;* RESULTAT
			Wt_LIG = Wt_LIG + 2 
			Office_WriteString(Wt_LIG, 2, "RESULTAT")
			Wi = 11
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 2 
			Wi = 12
			Office_WriteString(Wt_LIG, 2, "Comptes Spéciaux")
			Imprimer_Ligne_Recap 
		;*
			Wt_LIG = Wt_LIG + 1 
			Wi = 13
			Office_WriteString(Wt_LIG, 2, "Comptes Analytiques")
			Imprimer_Ligne_Recap 
			
		;* TOTAUX 
			Wt_LIG = Wt_LIG + 4 
			Office_WriteString(Wt_LIG, 2, "TOTAUX COMPTABILITE")
			Wi = 14
			Imprimer_Ligne_Recap 

	Endif

Endp


;*
Procedure Bloc_TotalGeneral_1_Ap
;*
; CIAG Août 2016 - PJO : Balance Sortie Excel 
;*
Beginp
	Standard.Bloc_TotalGeneral_1_Ap

	If c3xq.Choix(9) = 2
		; Ecriture feuille Excel 
		Wt_LIG = Wt_LIG + 2 
		Office_WriteString(Wt_LIG, 2, "TOTAL GENERAL")
		Office_WriteFormat(Wt_LIG, 3, X4.BalMtRep(4), "Numeric", 2)
		If C3xq.Choix(6) = 2
			Office_WriteString(Wt_LIG, 4, "Cr")
		Elsif C3xq.Choix(6) = 1
			Office_WriteString(Wt_LIG, 4, "Db")
		Endif
		Office_WriteFormat(Wt_LIG, 5, X4.BalMtDb(4), "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 6, X4.BalMtCr(4), "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 7, X4.BalMtSldDb(4), "Numeric", 2)
		Office_WriteFormat(Wt_LIG, 8, X4.BalMtSldCr(4), "Numeric", 2)
	Endif

Endp
