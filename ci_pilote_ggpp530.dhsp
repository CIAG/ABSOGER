		G3XZ.MOTCLE = fstring('U',G3XZ.MOTCLE) 
		G3XZ.FIC =	chaine ; Fichier qui sera transmis à G3XQ par le programme ggpp530
		
		ping("G3XZ", G3XZ)
		ping("G3XQ", G3XQ)
		ping("XQ", G3XQ)
		Ping('ZECHANGE_GTPCE', GTPCE)	; Obligatoire dans ce tunnel même si aucune info est utilisées
		programcall( Prg, wpfNoClient = True )
	endif
	
programexit
